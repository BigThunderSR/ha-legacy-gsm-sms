{
  "extends": [
    "config:base"
  ],
  "labels": [
    "Renovate",
    "dependencies"
  ],
  "packageRules": [
    {
      "matchManagers": ["pip_requirements", "pip_setup", "pipenv", "poetry"],
      "labels": ["python"]
    },
    {
      "matchManagers": ["github-actions"],
      "labels": ["github_actions"]
    },
    {
      "matchManagers": ["dockerfile"],
      "labels": ["dockerfile"]
    },
    {
      "matchManagers": ["npm", "yarn", "pnpm", "nvm"],
      "labels": ["javascript", "nvm"]
    }
  ]
}